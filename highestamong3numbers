import java.util.Scanner;

public class highestamong3numbers {

	public static void main(String args[])
	{
		int a,b,c;
		Scanner s= new Scanner(System.in);
		System.out.println("enter the values of a,b,c");
		a=s.nextInt();
		b=s.nextInt();
		c=s.nextInt();
		
		if(a>b && a>c)
		{
			
				System.out.println("The highest among a,b,c is a:" +a);
			}
			
			
   	
		else if (b>a && b>c)
		{
				System.out.println("The highest among a,b,c is b:" +b);
				
		}
			
		else
			System.out.println("The highest among a,b,c is c:" +c);
		}
}
